<%= form_with url: "/games/#{@game.id}", method: :patch, local: true  do |form|  %>
  <%= form.label :name%>
  <%= form.text_field :name%><br>
  <%= form.label :won, "Won" %>
  <%= form.radio_button :won, value=true %><br>
  <%= form.label :lost, "Lost"%>
  <%= form.radio_button :won, value=false%><br>
  <%= form.number_field :number_of_moves%><br>
  <%= form.submit "Submit" %>
<% end %>