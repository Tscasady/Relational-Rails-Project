<%= form_with url: "/players/#{@player.id}", method: :patch, local: true  do |form|  %>
  <%= form.label :name%>
  <%= form.text_field :name%><br>
  <%= form.label :Rating%>
  <%= form.text_field :rating%><br>
  <%= form.label :age%>
  <%= form.text_field :age%><br>
  <%= form.submit "Submit" %>
<% end %>