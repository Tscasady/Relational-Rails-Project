<%= form_with url: '/players', method: :post, local: true  do |form|  %>
  <%= form.label :name%>
  <%= form.text_field :name%><br>
  <%= form.label :Rating%>
  <%= form.text_field :Rating%><br>
  <%= form.label :age%>
  <%= form.text_field :age%><br>
  <%= form.submit "Create Player" %>
<% end %>